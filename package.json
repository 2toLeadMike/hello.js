{
  "name": "hellojs",
  "version": "1.15.1",
  "description": "A clientside Javascript library for standardizing requests to OAuth2 web services (and OAuth1 - with a shim)",
  "homepage": "https://adodson.com/hello.js",
  "main": "dist/hello.all.js",
  "repository": {
    "type": "git",
    "url": "git@github.com/MrSwitch/hello.js"
  },
  "keywords": [
    "oauth",
    "oauth1.0",
    "oauth2",
    "api",
    "facebooks",
    "google",
    "windows",
    "linkedin",
    "twitter"
  ],
  "author": {
    "name": "Andrew Dodson",
    "email": "andrewjdodson@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrSwitch/hello.js/issues"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-assign"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "debug": "gulp watch",
    "start": "npm run compile:hello && npm run compile:hello.all && npm run compile:hello.min && npm run compile:hello.all.min",
    "compile:hello": "browserify src/hello.js -t [babelify --sourceMap] -o dist/hello.js",
    "compile:hello.min": "browserify src/hello.js -d -t [babelify --sourceMap] | exorcist dist/hello.min.js.map > dist/hello.min.js",
    "compile:hello.all": "browserify src/hello.all.js -t [babelify --sourceMap] -o dist/hello.all.js",
    "compile:hello.all.min": "browserify src/hello.all.js -d -t [babelify --sourceMap] | exorcist dist/hello.all.min.js.map > dist/hello.all.min.js",
    "test": "gulp test && npm run test:browserstack",
    "test:browserstack": "bash browserstack.sh",
    "test:browserstack:env": "source .env.sh && npm run test:browserstack"
  },
  "devDependencies": {
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "browserstack-runner": "^0.4.4",
    "eslint": "^3.10.2",
    "expect.js": "^0.3.1",
    "gulp": "^3.9.0",
    "gulp-mocha-phantomjs": "^0.10.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.6",
    "localhost": "^0.1.2",
    "mocha": "^3.1.2",
    "shunt": "^0.1.6",
    "sinon": "^1.17.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "exorcist": "^0.4.0",
    "tricks": "^1.5.3"
  }
}